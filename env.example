# Configurações do Supabase
VITE_SUPABASE_URL=http://localhost:54321
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_DB_PASSWORD=your-super-secret-and-long-postgres-password

# Configurações do Google Gemini AI
VITE_GOOGLE_GEMINI_API_KEY=your-gemini-api-key

# Configurações da Evolution API (WhatsApp)
VITE_EVOLUTION_API_URL=http://localhost:8080
VITE_EVOLUTION_API_KEY=your-evolution-api-key
VITE_EVOLUTION_INSTANCE_NAME=your-instance-name

# Configurações do Grafana
GRAFANA_PASSWORD=admin

# Configurações do Nginx (para autenticação básica)
NGINX_USER=admin
NGINX_PASSWORD=admin123

# Configurações de produção (altere para seu domínio)
DOMAIN=localhost
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# URL do WebSocket (prioridade sobre VITE_EVOLUTION_API_URL se definido)
VITE_WEBSOCKET_URL=http://localhost:3001

# Servidor de Webhooks
WEBHOOK_PORT=3001
WEBHOOK_BASE_URL=http://localhost:3001
FRONTEND_URL=http://localhost:5173